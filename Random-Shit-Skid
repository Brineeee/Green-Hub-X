local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Brineeee/Kavo-GT/main/lua"))()

local GreenTheme = {
      SchemeColor = Color3.fromRGB(120, 200, 0),
      Background = Color3.fromRGB(0, 0, 0),
      Header = Color3.fromRGB(0, 0, 0),
      TextColor = Color3.fromRGB(255,255,255),
      ElementColor = Color3.fromRGB(0, 0, 0)
} 

local Window = Library.CreateLib("Green X Hub - v0.1", GreenTheme)

local Tab1 = Window:NewTab("Home")
local Tab1Section = Tab1:NewSection("Credits //")
local Tab2 = Window:NewTab("Hubs")
local Tab2Section = Tab2:NewSection("Hubs //")

Tab1Section:NewDropdown("Credits <3", "Scriptor : mech#0945 [ Discord ]", {"Scriptor : mech#0945 [ Discord ]", "UI : Kavo UI [ Edited ]", "Protected : MoonSecV2 [ Discord ]"}, function()
    setclipboard("https://linktr.ee/brine777") 
    setclipboard("Obfuscation -> https://discord.gg/JWHmpyKWy5") 
end)

